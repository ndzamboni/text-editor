{
  "version": 2,
  "builds": [
    {
      "src": "Develop/server/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "Develop/client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "client/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server/server.js"
    }
  ]
}
